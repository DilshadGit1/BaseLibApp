<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:id="@+id/lay1"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginRight="@dimen/_37sdp"
        android:paddingBottom="10dp"
        android:layout_centerVertical="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/text_property"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text="Holding No."
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_propertyno"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text=""
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/text_appno"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/cc_montserrat_bold"
                    android:text="Application No."
                    android:textColor="@color/view_text"
                    android:textSize="@dimen/_10sdp" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_applictnno"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text=""
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/text_district"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text="District"
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_district"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text=""
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/text_occupancytype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text="Ward"
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/tv_ward"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text=""
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/text_ownername"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text="Owner Name"
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/ownername"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/view_text"
                    android:text=""
                    android:textSize="@dimen/_10sdp"
                    android:fontFamily="@font/cc_montserrat_bold" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/black_dark"
        android:layout_below="@id/lay1"
        android:layout_marginTop="@dimen/_50sdp"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_10sdp" />
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/layout_dots_click"
        android:orientation="horizontal"
        android:layout_width="@dimen/_130sdp"
        android:layout_height="@dimen/_45sdp"
        android:layout_alignParentRight="true"
        android:weightSum="3"
        android:background="@drawable/popup_background_pink">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_weight="1"
            android:gravity="center">

        </RelativeLayout>
        <View
            android:layout_width="@dimen/_1sdp"
            android:layout_height="@dimen/_22sdp"
            android:layout_margin="@dimen/_5sdp"
            android:background="@color/black_dark"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_5sdp"
            android:layout_weight="1"
            android:gravity="center">
            <ImageView
                android:id="@+id/edit"
                android:layout_width="@dimen/_22sdp"
                android:layout_height="@dimen/_22sdp"
                android:src="@drawable/edit_profile" />
        </RelativeLayout>
        <View
            android:id="@+id/view3"
            android:layout_width="@dimen/_1sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_margin="@dimen/_5sdp"
            android:background="@color/black_dark" />
        <RelativeLayout
            android:id="@+id/laydelete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_5sdp"
            android:layout_weight="1"
            android:gravity="center">
            <ImageView
                android:id="@+id/delete"
                android:layout_width="@dimen/_22sdp"
                android:layout_height="@dimen/_22sdp"
                android:src="@drawable/delete_icon" />
        </RelativeLayout>
    </LinearLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_1sdp"
        android:layout_alignParentRight="true">
        <RelativeLayout
            android:id="@+id/laylog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:layout_marginTop="@dimen/_3sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_alignParentRight="true">
            <ImageView
                android:id="@+id/ivlog"
                android:layout_width="@dimen/_25sdp"
                android:layout_height="@dimen/_25sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:src="@drawable/logs" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/bluethlyoutpreview"
            android:layout_below="@+id/laylog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/_7sdp"
            android:layout_marginTop="@dimen/_5sdp">
            <ImageView
                android:id="@+id/viewblutoothpreview"
                android:layout_width="@dimen/_24sdp"
                android:layout_height="@dimen/_24sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:src="@drawable/print_icon" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/laydot"
            android:layout_below="@+id/bluethlyoutpreview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_7sdp">
            <ImageView
                android:visibility="gone"
                android:id="@+id/nonactive_dots"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/non_active_dot_ver"/>

            <ImageView
                android:layout_width="@dimen/_22sdp"
                android:layout_height="@dimen/_22sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:layout_marginBottom="@dimen/_5sdp"
                android:src="@drawable/view_icon" />
            <ImageView
                android:visibility="gone"
                android:id="@+id/active_dots"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/active_dots_pink"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/bluethlyout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_marginTop="@dimen/_3sdp"
            android:layout_below="@+id/laydot">
            <ImageView
                android:id="@+id/print_demand"
                android:layout_width="@dimen/_24sdp"
                android:layout_height="@dimen/_24sdp"
                android:layout_marginStart="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_2sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:src="@drawable/print_icon" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/pdfdoenloadlayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3sdp"
            android:layout_marginRight="@dimen/_4sdp"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/bluethlyout">
            <ImageView
                android:id="@+id/pdf_demand"
                android:layout_width="@dimen/_26sdp"
                android:layout_height="@dimen/_26sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:src="@drawable/pdf_dowload" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_alignParentRight="true"
            android:visibility="gone"
            android:layout_below="@+id/pdfdoenloadlayout">
            <ImageView
                android:id="@+id/viewpay"
                android:layout_width="@dimen/_25sdp"
                android:layout_height="@dimen/_25sdp"
                android:src="@drawable/paynow1" />
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
</layout>
